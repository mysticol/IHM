/* Dolphin */
DROP TABLE booking;
DROP TABLE planing;

CREATE TABLE booking (
                        bookingId INT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
                        startTime TIMESTAMP,
                        endTime TIMESTAMP,
                        instructorName VARCHAR(30),
                        adress VARCHAR(30),
                        price DOUBLE,

                        bookingName VARCHAR(30),
                        hotelName VARCHAR(30),
                        requestDate DATE,
                        numberOfDivers INT
                      );

CREATE TABLE planing (
                        jour VARCHAR(2) PRIMARY KEY,
                        instructorName VARCHAR(30)
                      );

