<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://j2ee.netbeans.org/wsdl/HotelModule/DivingBooking" xmlns:ns2="http://enterprise.netbeans.org/bpel/HotelModule/hotelModule" xmlns:ns3="http://enterprise.netbeans.org/bpel/ProcessManagerSimple/processManagerSimple" xmlns:ns4="http://enterprise.netbeans.org/bpel/mockWreckAndCaveDivingAgency/mockWreckAndCaveDivingAgency" xmlns:ns5="http://enterprise.netbeans.org/bpel/mockLunchService/mockLunchService" xmlns:ns6="http://enterprise.netbeans.org/bpel/mockSharkDivingAgency/mockSharkDivingAgency" xmlns:ns7="http://enterprise.netbeans.org/bpel/mockTaxiService/mockTaxiService" xmlns:ns8="http://enterprise.netbeans.org/bpel/mockDolphinDivingAgency/mockDolphinDivingAgency" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>DivingBookingCompositeApp</name>
            <description>Represents the Service Assembly of DivingBookingCompositeApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-mockWreckAndCaveDivingAgency</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>mockWreckAndCaveDivingAgency.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-HotelModule</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>HotelModule.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-mockSharkDivingAgency</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>mockSharkDivingAgency.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-mockLunchService</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>mockLunchService.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-ProcessManagerSimple</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>ProcessManagerSimple.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-mockTaxiService</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>mockTaxiService.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-mockDolphinDivingAgency</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>mockDolphinDivingAgency.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>DivingBookingCompositeApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="DivingBookingPortTypeBindingPort" service-name="ns1:DivingBookingService"/>
                <provider endpoint-name="DivingBookingPortTypeRole_myRole" service-name="ns2:Formulaire"/>
            </connection>
            <connection>
                <consumer endpoint-name="WACDivingAgencyPortTypeRole_partnerRole" service-name="ns3:WACAgency"/>
                <provider endpoint-name="WACDivingAgencyPortTypeRole_myRole" service-name="ns4:ProcessManager"/>
            </connection>
            <connection>
                <consumer endpoint-name="ScubaDivingPortTypeRole_partnerRole" service-name="ns2:ProcessManager"/>
                <provider endpoint-name="ScubaDivingPortTypeRole_myRole" service-name="ns3:ProcessManager"/>
            </connection>
            <connection>
                <consumer endpoint-name="lunchServicePortTypeRole_partnerRole" service-name="ns3:LunchService"/>
                <provider endpoint-name="lunchServicePortTypeRole_myRole" service-name="ns5:ProcessManager"/>
            </connection>
            <connection>
                <consumer endpoint-name="sharkDivingAgencyPortTypeRole_partnerRole" service-name="ns3:SharkAgency"/>
                <provider endpoint-name="sharkDivingAgencyPortTypeRole_myRole" service-name="ns6:ProcessManager"/>
            </connection>
            <connection>
                <consumer endpoint-name="taxiServicePortTypeRole_partnerRole" service-name="ns3:TaxiService"/>
                <provider endpoint-name="taxiServicePortTypeRole_myRole" service-name="ns7:ProcessManager"/>
            </connection>
            <connection>
                <consumer endpoint-name="dolphinDivingAgencyPortTypeRole_partnerRole" service-name="ns3:DolphinAgency"/>
                <provider endpoint-name="dolphinDivingAgencyPortTypeRole_myRole" service-name="ns8:ProcessManager"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
