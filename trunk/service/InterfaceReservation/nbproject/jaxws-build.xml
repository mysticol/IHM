<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:webproject2="http://www.netbeans.org/ns/web-project/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-client-Reception" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/Reception"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/Reception" destdir="${build.generated.dir}/jax-wsCache/Reception" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/Reception/wsdl/Reception.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/Users/JulienSambre/Documents/NetBeansProjects/mockProcess/src/jbiasa/mockReception/Reception.wsdl" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-service-references/Reception/wsdl/Reception.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/Reception"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/Reception">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-Reception" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/org/netbeans/j2ee/wsdl/mockreception/reception"/>
        <delete dir="${build.generated.dir}/jax-wsCache/Reception"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-Reception"/>
</project>
