<?xml version="1.0" encoding="UTF-8"?>
<process
    name="clientManager"
    targetNamespace="http://enterprise.netbeans.org/bpel/ClientManager/clientManager"
    xmlns:tns="http://enterprise.netbeans.org/bpel/ClientManager/clientManager"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://xml.netbeans.org/schema/types">
    <import namespace="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" location="ClientManagerRequest.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="Reservation" xmlns:tns="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" partnerLinkType="tns:ClientManagerRequest" myRole="ClientManagerRequestPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="ClientManagerRequestOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" messageType="tns:ClientManagerRequestOperationResponse"/>
        <variable name="ClientManagerRequestOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" messageType="tns:ClientManagerRequestOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="Reservation" operation="ClientManagerRequestOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" portType="tns:ClientManagerRequestPortType" variable="ClientManagerRequestOperationIn"/>
        <assign name="AssignMok">
            <copy>
                <from>concat($ClientManagerRequestOperationIn.request/ns0:Nom, '-', $ClientManagerRequestOperationIn.request/ns0:Prenom, '000')</from>
                <to variable="ClientManagerRequestOperationOut" part="IdClient"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="Reservation" operation="ClientManagerRequestOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/ClientManager/ClientManagerRequest" portType="tns:ClientManagerRequestPortType" variable="ClientManagerRequestOperationOut"/>
    </sequence>
</process>


